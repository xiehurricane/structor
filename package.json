  },
  "homepage": "https://github.com/ipselon/structor#readme",
  "bin": {
    "structor": "structor.js"
  },
  "engines": {
    "node": ">=0.12.2"
  },
  "dependencies": {
    "babel-core": "5.8.25",
    "babel-plugin-react-transform": "1.1.1",
